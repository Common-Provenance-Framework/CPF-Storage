# Stores an organization

@organization_id=myorg_3


POST http://{{$dotenv TRUSTED_PARTY_HOST}}:{{$dotenv TRUSTED_PARTY_PORT}}/api/v1/organizations/{{organization_id}} HTTP/1.1
Accept: application/json
Content-Type: application/json

{
  "organizationId": "{{organization_id}}",
  "clientCertificate": "{{$dotenv CLIENT_CERT}}",
  "intermediateCertificates": [
    "{{$dotenv INTERMEDIATE_CERT_1}}",
    "{{$dotenv INTERMEDIATE_CERT_2}}"
    ],
  "clearancePeriod": 30
}